{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["./src/*"],
      "@/assets/*": ["./src/assets/*"],
      "@/components/*": ["./src/components/*"],
      "@/pages/*": ["./src/pages/*"],
      "@/context/*": ["./src/context/*"],
      "@/hooks/*": ["./src/hooks/*"],
      "@/routes/*": ["./src/routes/*"],
      "@/styles/*": ["./src/styles/*"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules", "build"]
}
